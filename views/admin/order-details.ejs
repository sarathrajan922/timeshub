    <script>

let changeClass = (status)=>{
         

         switch(status){

             case  'pending' :
             return 'badge rounded-pill alert-primary'

             case 'Placed' :
             return "badge rounded-pill alert-primary"
              
             case 'confirmed' :
             return 'badge rounded-pill alert-success'

             case 'shipped' :
             return 'badge rounded-pill alert-dark'

             case 'out of delivery' :
             return 'badge rounded-pill alert-info'

             case 'Completed' :
             return 'badge rounded-pill alert-success'

             case 'Cancelled' :
             return 'badge rounded-pill alert-danger'
         }
     }
//         let data = document.getElementById('inputbox').value
//         $(document).ready(function() {
//   $('#status-select').on('change', function() {
//     var status = $(this).val();
    function changeStatus(status, orderId){

        
        $.ajax({
      url: '/admin/update-status',
      type: 'POST',
      data: { 
        status: status,
        id: orderId,
     },
      success: function(response) {

          let className = changeClass(status)
          console.log(className)
        
        $(`#${orderId}`).removeClass().addClass(className);
          
        

         document.getElementById(orderId).innerHTML = status
        // console.log(response);
      },
      error: function(xhr, status, error) {
        console.log(error);
      }
    });
    }
    
 
    </script>
