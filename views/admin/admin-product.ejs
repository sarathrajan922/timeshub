                                </a>
                            </div>

                            

                            <div class="col-lg-2 col-sm-2 col-4 col-price"> <span><%= locals.productPrice[i] %></span> </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-status">
                                <span class="badge rounded-pill alert-secondary"><%= locals.array[i].category %></span>
                            </div>
                            <div class="col-lg-1 col-sm-2 col-4 col-date">
                                <span><%= locals.array[i].model %></span>
                            </div>




                            <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                                <a href="/admin/product-edit/<%= locals.array[i]._id %>" class="btn btn-sm font-sm rounded btn-brand">
                                    <i class="material-icons md-edit"></i> Edit
                                </a>
                                <a class="btn btn-sm font-sm btn-light rounded" onclick="deleteProduct('<%= locals.array[i]._id %>')">
                                    <i class="material-icons md-delete_forever"></i> Delete
                                </a>
                            </div>







                        </div> <!-- row .// -->
                    </article> <!-- itemlist  .// -->
                    

                    <% } %>
