    <script>
        function deleteProduct(prodId){

            swal({
                title: "Are you sure?",
                icon: "warning",
                buttons: true,
                dangerMode: true,
                })
                .then((willDelete) => {
                if (willDelete) {

                    productDelete(prodId)

                    swal("Poof! Your product Data has been deleted!", {
                    icon: "success",
                    });
                } else {
                    swal("Your product Data is safe!");
                }
                });

        }




        
        function productDelete(id){
            //ajax here

            $.ajax({
                url: '/admin/product-delete',
                type: 'post',
                data : {
                    id
                },
                success: (response)=>{
                    if(response.status){
                        location.reload()
                    }
                }
            })
                

        }
    </script>













